<tbody>
    <% @rooms.each do |room| %> 
        <tr>
          <td><%= link_to room.roomname,room_path(room)%></td>
          <td><%= room.introduction %></td>
          <td><%= room.price %></td>
          <td><%= room.address %></td>    
          <td><%= room.updated_at.to_s(:datetime_jp) %></td>
          <td><%= room.created_at.to_s(:datetime_jp)%></td>
          <td><%= link_to room_path(room) do %>
          <% if room.roomimg? %> 
            <%= image_tag room.roomimg.url %>
          <% end %>
        <% end %></td>
       <td><%= link_to "削除",room, method: :delete, data: { confirm: "本当に削除しますか？" } %></td>
       <td><%= room.user_id %></td>
        </tr>    
       <% end %>
  </tbody>
</table>