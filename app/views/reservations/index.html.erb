<div class="container mt-5">
  <div class="row">
    <div class="col-10 mx-auto">
      <table class="table table-hover">
        <thead class="thead-dark">
          <tr>
            <th scope="col">部屋名</th>
            <th scope="col"></th>
            <th scope="col"></th>
          </tr>
        </thead>
        <tbody>
          <% @reservations.each do |reser| %>
            <tr>
              <td><%= link_to reser.roomname, reservation_path(reser) %></td>
              <td><%= reser.room.start_day %></td>
              
            
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>

  